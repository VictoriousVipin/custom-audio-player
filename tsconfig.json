{
    "compilerOptions": {
        "target": "ES6",
        "module": "commonjs",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "outDir": "./dist",
        "rootDir": "./src",
        "lib": ["DOM", "ES6", "DOM.Iterable", "ScriptHost"]
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"]
}
